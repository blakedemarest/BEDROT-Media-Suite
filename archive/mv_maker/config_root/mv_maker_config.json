{
    "transcription_service": "elevenlabs",
    "elevenlabs_api_key": "",
    "elevenlabs_model": "scribe_v1",
    "audio_events": false,
    "diarize_speakers": true,
    "detect_speakers": true,
    "max_speakers": 32,
    "whisper_model": "base",
    "language": "auto",
    "output_formats": [
        "srt",
        "vtt",
        "simple",
        "mp4"
    ],
    "output_directory": "",
    "caption_max_length": 42,
    "caption_max_duration": 7.0,
    "font_size": 24,
    "font_color": "#FFFFFF",
    "background_color": "#000000",
    "background_opacity": 0.7,
    "position": "bottom",
    "margin": 20,
    "device": "auto",
    "batch_size": 16,
    "compute_type": "float16",
    "beam_size": 5,
    "best_of": 5,
    "patience": 1.0,
    "length_penalty": 1.0,
    "temperature": 0.0,
    "compression_ratio_threshold": 2.4,
    "logprob_threshold": -1.0,
    "no_speech_threshold": 0.6,
    "condition_on_previous_text": true,
    "initial_prompt": null,
    "word_timestamps": true,
    "prepend_punctuation": "\"'([{-",
    "append_punctuation": "\"'.)]}",
    "last_video_path": "",
    "last_output_path": "",
    "mp4_overlay_quality": "high",
    "mp4_overlay_bitrate": "2M",
    "font_family": "arial",
    "available_fonts": ["arial", "helvetica", "verdana", "roboto", "open_sans", "impact"],
    "subtitle_border": 2,
    "subtitle_shadow": true,
    "position_x": 50,
    "position_y": 85,
    "position_unit": "percent",
    "text_align": "center",
    "vertical_align": "bottom",
    "aspect_ratio": "16:9",
    "video_resolution": "1080p",
    "background_type": "solid",
    "background_value": "#000000",
    "enable_preview": true,
    "preview_update_delay": 50
}